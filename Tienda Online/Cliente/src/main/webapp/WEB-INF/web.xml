<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Hola</servlet-name>
		<servlet-class>prueba.Hola</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hola</servlet-name>
		<url-pattern>/hola.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>tienda.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>tienda.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Registro</servlet-name>
		<servlet-class>tienda.RegistroServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registro</servlet-name>
		<url-pattern>/registro.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProcesarPedido</servlet-name>
		<servlet-class>tienda.ProcesarPedidoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProcesarPedido</servlet-name>
		<url-pattern>/ProcesarPedido.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CambiarDatos</servlet-name>
		<servlet-class>tienda.CambiarDatosServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CambiarDatos</servlet-name>
		<url-pattern>/CambiarDatos.html</url-pattern>
	</servlet-mapping>
</web-app>
